        "start:windows": "bin/run-server.bat",
        "start": "@start:linux",
        "test:crud": "php vendor/bin/codecept run Crud",
        "test:codecept": "php vendor/bin/codecept run",
        "test": ["@test:cs", "@test:codecept"]
